<div class="col-md-6 col-md-offset-3">
  <div class="panel panel-default">
    <div class="panel panel-title">
      <h1>Editing Pin</h1>
    </div>
    <div class="panel panel-body">
      <%= image_tag @pin.image.url %>
      <%= render 'form' %>
    </div>
    <div class="panel panel-footer">
      <%= link_to 'Show', @pin %> |
      <%= link_to 'Back', pins_path %>
    </div>
  </div>
</div>
