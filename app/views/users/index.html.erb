<div class="col-md-6 col-md-offset-3">
  <div class="panel panel default">
    <div class=" panel panel header">
      <h1>Users index</h1>
    </div>
    <div class="panel panel body">
      <% @users.each do |user| %>
        <h6>Username: <%= user.username %></h6><br>
        <h6>User Email: <%= user.email %></h6><br/>
        <h6>number of times signed in: <%= user.sign_in_count %></h6><br>
        <h6>Last signed in: <%= user.last_sign_in_at %></h6><br>
        <hr>
      <% end %>
    </div>
  </div>
</div>
